Xcursor.theme: G5-Mouse

URxvt*boldFont: xft:terminus-7:style=Bold
#URxvt*boldFont: xft:drift:style=Bold
#URxvt*boldFont: -*-terminus-*-*-*-*-*-*-*-*-*-*-*-*
URxvt*font: xft:terminus-7:style=Regular
#URxvt*font: xft:drift:style=Regular
#URxvt*font: -*-terminus-*-*-*-*-*-*-*-*-*-*-*-*
URxvt*buffered: true
URxvt*cursorUnderline: true
URxvt*geometry: 84x22
URxvt*iso14755_52: false
URxvt*jumpScroll: true
URxvt*perl-ext-common: matcher
URxvt*urlLauncher: /usr/bin/firefox
URxvt*scrollBar: false
URxvt*skipScroll: true
URxvt*cutchars: "()*,<>[]{}|'
URxvt*urgentOnBell: true
#URxvt*transparent:          true
#URxvt*shading:              10
#URxvt*inheritPixmap:        true
URDesktop*transparent: true
URDesktop*inheritPixmap: true

URxvt*internalBorder: 6

*background:    #111111
*foreground:    #cccccc

! Black

*color0:    #404040
*color8:    #505050

! Red

*color1:    #CE5666
*color9:    #952743

! Green

*color2:    #80A673
*color10:    #97D599

! Yellow

*color3:    #E0BC93
*color11:    #FFCAA2

! Blue

*color4:    #778BAF
*color12:    #5A667F

! Magenta

*color5:    #94738C
*color13:    #D8ADCE

! Cyan

*color6:    #B5D2DD
*color14:    #82A9CC

! White

*color7:    #d2d2d2
*color15:    #ffffff

